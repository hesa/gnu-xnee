void
on_action_button_clicked               (GtkButton       *button,
                                        gpointer         user_data)
{
    printf("on_action_button_clicked\n");

    xnee_data *xd;
    
    xnee_init(xd, PACKAGE);
    xnee_record_init(xd);
    xnee_set_recorder(xd);
    xnee_add_range_str(xd, XNEE_DELIVERED_EVENT, "KeyPress");
    xnee_setup_display(xd);
    xnee_setup_recording(xd);
    xnee_print_sys_info(xd, "gnee.xnee");
    xnee_print_xnee_settings(xd, "gnee.xnee");
    xnee_record_print_record_range(xd, "gnee.xnee");
    xnee_record_loop(xd);
}


